{
  "name": "kratelabs-rest",
  "version": "1.0.0",
  "description": "## Install",
  "main": "server.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "pretest": "npm run lint || true",
    "test": "ava --verbose",
    "start": "./node_modules/nodemon/bin/nodemon.js ./node_modules/babel-cli/bin/babel-node.js app/server.js",
    "lint": "./node_modules/eslint/bin/eslint.js app && ./node_modules/eslint/bin/eslint.js test",
    "autostart": "nohup ./node_modules/nodemon/bin/nodemon.js ./node_modules/babel-cli/bin/babel-node.js app/server.js </dev/null &"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KrateLabs/KrateLabs-REST.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KrateLabs/KrateLabs-REST/issues"
  },
  "homepage": "https://github.com/KrateLabs/KrateLabs-REST#readme",
  "dependencies": {
    "base-64": "^0.1.0",
    "body-parser": "^1.15.1",
    "core-js": "^2.4.0",
    "express": "^4.13.4",
    "express-jwt": "^3.4.0",
    "express-useragent": "^0.2.4",
    "form-data": "^1.0.0-rc4",
    "humps": "^1.1.0",
    "isomorphic-fetch": "^2.2.1",
    "js-base64": "^2.1.9",
    "jsonwebtoken": "^7.0.1",
    "lodash": "^4.13.1",
    "mongoose": "^4.4.20",
    "mongoose-auto-increment": "^5.0.1",
    "multer": "^1.1.0",
    "node-uuid": "^1.4.7",
    "utf8": "^2.1.1",
    "validator": "^5.4.0"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.0.1",
    "ignore-styles": "^4.0.0",
    "nock": "^8.0.0",
    "nodemon": "^1.9.2"
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "babel": "inherit",
    "concurrency": 5,
    "failFast": false,
    "tap": true,
    "require": [
      "babel-register",
      "ignore-styles"
    ]
  }
}
